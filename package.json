{
  "name": "vx10-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:reset": "prisma migrate reset",
    "setup:postgres": "powershell -ExecutionPolicy Bypass -File ./setup_postgres.ps1",
    "setup:complete": "bash setup-vx10-complete.sh",
    "setup:update": "bash setup-vx10-complete.sh --update",
    "git:pull": "git fetch origin && git pull origin master",
    "git:push": "git add . && git commit -m \"Auto-commit: $(date)\" && git push origin master",
    "git:sync": "npm run git:pull && npm ci && npm run prisma:generate && npm run build && npm run git:push",
    "deploy:dev": "npm ci && npm run prisma:generate && npm run prisma:push && npm run build",
    "deploy:prod": "npm ci --production && npm run prisma:generate && npm run prisma:migrate && npm run build",
    "postinstall": "npm run prisma:generate",
    "diagnose": "node scripts/diagnose-build.js",
    "diagnose:fix": "node scripts/diagnose-build.js --fix",
    "logs:clean": "rimraf logs",
    "logs:view": "tail -f logs/*.log",
    "health-check": "node -e \"require('./utils/logger').logger.checkServerHealth(); require('./utils/logger').logger.checkBuildIntegrity();\"",
    "clear-cache": "rimraf .next && npm cache clean --force",
    "supabase:reset": "npx supabase db reset",
    "supabase:migrate": "npx prisma migrate deploy",
    "supabase:seed": "node prisma/seed.js",
    "supabase:generate": "npx prisma generate && npx supabase gen types typescript --local > src/types/supabase.ts",
    "db:setup": "npm run supabase:migrate && npm run supabase:generate && npm run supabase:seed"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.38.4",
    "@prisma/client": "6.11.1",
    "next": "15.4.4",
    "react": "^19",
    "react-dom": "^19",
    "next-auth": "^5.0.0-beta.22",
    "lucide-react": "^0.468.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4",
    "class-variance-authority": "^0.7.1",
    "@mui/material": "^6.1.9",
    "@mui/joy": "^5.0.0-beta.52",
    "@mui/icons-material": "^6.1.9",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "sonner": "^1.7.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "prisma": "6.11.1",
    "rimraf": "6.0.1",
    "tw-animate-css": "1.3.4",
    "typescript": "^5",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.0"
  }
}
